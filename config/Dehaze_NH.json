{
    "name": "Dehaze_NH",
    "phase": "train", // train or val
    "gpu_ids": [
        0
    ],
    "path": { //set the path
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": null,
        "resume_stateH": "/mnt/workspace/workgroup/yuhu/code/DehazeDDPM/pretrained_PreNet_pth/NH_net_g_80000.pth"
    },
    "datasets": {
        "train": {
            "name": "ITS",
            "img_sizeH": 560,  // whether need LR img
            "img_sizeW": 752,  // whether need LR img
            "datarootlq": "./data/NH-HAZE/hazy_train",
            "dataroothq": "./data/NH-HAZE/gt_train",
            "datatype": "img", //lmdb or img, path of img files
            "batch_size": 1,
            "num_workers": 0,
            "use_shuffle": true,
            "len": -1 // -1 represents all data used in train
        },
        "val": {
            "name": "SOTS",
            "img_sizeH": 1200, //576, //512 , // whether need LR img
            "img_sizeW": 1600, //768, //672 , // whether need LR img
            "datarootlq": "./data/NH-HAZE/hazy_test",
            "dataroothq": "./data/NH-HAZE/gt_test",
            "datatype": "img", //lmdb or img, path of img files
            "len": 5 // data length in validation
        }
    },
    "model": {
        "which_model_G": "sr3", // use the ddpm or sr3 network structure
        "finetune_norm": false,
        "unet": {
            "in_channel": 7,
            "out_channel": 3,
            "inner_channel": 64,
            "channel_multiplier": [
                1,
                2,
                4,
                8,
                8
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 2,
            "dropout": 0.2
        },
        "beta_schedule": { // use munual beta_schedule for acceleration
            "train": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "val": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 512,
            "channels": 3, //sample channel
            "conditional": true // unconditional generation or unconditional generation(super_resolution)
        }
    },
    "train": {
        "n_iter": 2000000,
        "val_freq": 1e4,
        "save_checkpoint_freq": 1e4,
        "print_freq": 500,
        "optimizer": {
            "type": "adam",
            "lr": 1e-4
        },
        "ema_scheduler": { // not used now
            "step_start_ema": 5000,
            "update_ema_every": 1,
            "ema_decay": 0.9999
        }
    },
    "wandb": {
        "project": "Dehaze"
    }
}